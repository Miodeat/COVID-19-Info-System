<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>getEpidemicInfoServlet</servlet-name>
        <servlet-class>servlets.getEpidemicInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getEpidemicInfoServlet</servlet-name>
        <url-pattern>/getEpidemicInfoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getCollectiveRationalityInfoServlet</servlet-name>
        <servlet-class>servlets.getCollectiveRationalityInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getCollectiveRationalityInfoServlet</servlet-name>
        <url-pattern>/getCollectiveRationalityInfoServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>getEpidemicTimeSeriesServlet</servlet-name>
        <servlet-class>servlets.getEpidemicTimeSeriesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getEpidemicTimeSeriesServlet</servlet-name>
        <url-pattern>/getEpidemicTimeSeriesServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getTreeDataServlet</servlet-name>
        <servlet-class>servlets.getTreeDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getTreeDataServlet</servlet-name>
        <url-pattern>/getTreeDataServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InsertDataServlet</servlet-name>
        <servlet-class>servlets.InsertDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertDataServlet</servlet-name>
        <url-pattern>/InsertDataServlet</url-pattern>
    </servlet-mapping>
    
    <listener>
        <listener-class>services.InitService</listener-class>
    </listener>
</web-app>